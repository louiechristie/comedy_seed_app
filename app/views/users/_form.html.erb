<ul class="errors">
  <% @user.errors.full_messages.each do |error| %>
    <li>
      <%= error %>
    </li> 
  <% end %>
</ul>
<%= form_for(@user) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br />
  <%= f.label :email %>
  <%= f.text_field :email %><br />
  <%= f.label :user_image %>
  <%= f.file_field :user_image %><br />
  <%= f.label "Youtube video link" %>
  <%= f.text_field :video %><br />
  <%= f.label "Biography" %><br />
  <%= f.text_area :bio %><br />
  <%= f.submit %>
<% end %>